{
  "private": true,
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn format",
      "yarn lint"
    ]
  },
  "scripts": {
    "build": "dotenv -- turbo run build",
    "dev": "dotenv -- turbo run dev",
    "lint": "dotenv -- turbo run lint",
    "format": "dotenv -- prettier --write \"**/*.{ts,tsx,md}\"",
    "db:generate": "dotenv -- turbo run db:generate"
  },
  "devDependencies": {
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "prettier": "^2.5.1"
  },
  "name": "integra",
  "packageManager": "yarn@1.22.19",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "dotenv-cli": "^7.2.1",
    "turbo": "^1.10.12"
  }
}
