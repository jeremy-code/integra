generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model house {
  id                      String  @id @default(auto()) @map("_id") @db.ObjectId
  api_uri                 String
  at_large                Boolean
  contact_form            Json?
  cook_pvi                String?
  crp_id                  String?
  cspan_id                String?
  date_of_birth           String
  district                String
  dw_nominate             Float?
  facebook_account        String?
  fax                     Json?
  fec_candidate_id        String
  first_name              String
  gender                  String
  geoid                   String
  google_entity_id        String?
  govtrack_id             String?
  icpsr_id                String?
  id_                     String  @map("id")
  ideal_point             Json?
  in_office               Boolean
  last_name               String
  last_updated            String
  leadership_role         String?
  middle_name             String?
  missed_votes            Int?
  missed_votes_pct        Float?
  next_election           String?
  ocd_id                  String
  office                  String?
  party                   String
  phone                   String?
  rss_url                 String?
  seniority               String
  short_title             String
  state                   String
  suffix                  String?
  title                   String
  total_present           Int?
  total_votes             Int?
  twitter_account         String?
  url                     String
  votes_against_party_pct Float?
  votes_with_party_pct    Float?
  votesmart_id            String?
  youtube_account         String?
}

model senators {
  id                      String  @id @default(auto()) @map("_id") @db.ObjectId
  api_uri                 String
  contact_form            String?
  cook_pvi                Json?
  crp_id                  String
  cspan_id                String?
  date_of_birth           String
  dw_nominate             Float?
  facebook_account        String?
  fax                     String?
  fec_candidate_id        String
  first_name              String
  gender                  String
  google_entity_id        String
  govtrack_id             String
  icpsr_id                String?
  id_                     String  @map("id")
  ideal_point             Json?
  in_office               Boolean
  last_name               String
  last_updated            String
  leadership_role         String?
  lis_id                  String
  middle_name             String?
  missed_votes            Int
  missed_votes_pct        Float
  next_election           String
  ocd_id                  String
  office                  String?
  party                   String
  phone                   String?
  rss_url                 String?
  senate_class            String
  seniority               String
  short_title             String
  state                   String
  state_rank              String
  suffix                  String?
  title                   String
  total_present           Int
  total_votes             Int
  twitter_account         String?
  url                     String
  votes_against_party_pct Float
  votes_with_party_pct    Float
  votesmart_id            String?
  youtube_account         String?
}
