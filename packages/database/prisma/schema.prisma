generator client {
  provider = "prisma-client-js"
}

generator typegraphql {
  provider = "typegraphql-prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type OfficialSocialMedia {
  facebook String?
  twitter  String?
  youtube  String?
}

model official {
  id                      String              @id @default(auto()) @map("_id") @db.ObjectId
  at_large                Boolean?
  bioguide_id             String
  congressional_district  String?
  contact_form            String?
  cook_pvi                String?
  crp_id                  String
  cspan_id                String?
  date_of_birth           DateTime            @db.Date
  district                String?
  dw_nominate             Float?
  emails                  String[]
  fax                     String?
  fec_candidate_id        String
  first_name              String
  full_name               String
  gender                  String
  geoid                   String?
  google_entity_id        String?
  govtrack_id             String?
  icpsr_id                String?
  in_office               Boolean
  last_name               String
  last_updated            BigInt
  leadership_role         String?
  lis_id                  String?
  middle_name             String?
  missed_votes            Int?
  missed_votes_pct        Float?
  next_election           String?
  ocd_id                  String
  office                  String?
  party                   String
  phone                   String?
  rss_url                 String?
  senate_class            String?
  seniority               String
  short_title             String
  social_media            OfficialSocialMedia
  state                   String
  state_rank              String?
  suffix                  String?
  title                   String
  total_present           Int?
  total_votes             Int?
  url                     String
  votes_against_party_pct Float?
  votes_with_party_pct    Float?
  votesmart_id            String?
}
