{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [
    "$NODE_ENV",
    "$NEXT_PUBLIC_GOOGLE_MAPS_API_KEY",
    "$NEXT_PUBLIC_GRAPHQL_SERVER_URL",
    "$ANALYZE"
  ],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
